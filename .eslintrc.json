{
  "env": {
    "browser": true
  },
  "rules": {
    "max-len": [2, 200, 2, {"ignoreUrls": true}],
    "no-nested-ternary": 0,
    "object-property-newline": 0
  },
  "parserOptions": {
      "sourceType": "module",
      "ecmaVersion": 8,
      "ecmaFeatures": {
          "experimentalObjectRestSpread": true
      }
  }
}
